 public String toLowerCase(String s) {
        char [] temp = new char[s.length()];

         for(int i =0;i<s.length();i++){
             int a = (int) s.charAt(i);  
             if(65<=a && a<=90){
              temp[i]= (char)(a+32);
              }else temp[i]=s.charAt(i);
         }
         return String.valueOf(temp);
    }
